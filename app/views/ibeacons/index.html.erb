<div class="home-page_container col-xs-12">
<% @data.each do |data| %>
	<div class="workout-card col-xs-12 col-sm-4">
		<!--
		<span><span>X Coordinates: </span><%= data.x_motion %></span><br class="clr" />
		<span><span>Y Coordinates: </span><%= data.y_motion %></span><br class="clr" />
		<span><span>Z Coordinates: </span><%= data.z_motion %></span><br class="clr" />-->
      <h3 class="text-center"><i class="glyphicon glyphicon-th-list"><span> Workout: </span><%= data.exercise_name %></h3><br class="clr" />
      <hr />
      <span><i class="glyphicon glyphicon-flash"></i><span> Reps: </span><%= data.reps_counted %></span><br class="clr" />
      <span><i class="glyphicon glyphicon-fire"></i><span> Calories Burned: </span><%= data.reps_counted * 0.4918 %></span><br class="clr" />
      <span><i class="glyphicon glyphicon-heart"></i><span> Weight Lifted: </span><%= data.reps_counted * 20 %></span><br class="clr" />
	</div>
	<br class="clr" />
	<br class="clr" />
	<br class="clr" />
	<br class="clr" />
<% end %>
<br class="clr" /><br /><br />
</div>